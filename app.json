{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyAlLNaErfCFsLoGqbiihpo6EIQvKb1XIHo"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to show nearby rides."
      },
      "bundleIdentifier": "com.divijakavishvar.cabby" 
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAlLNaErfCFsLoGqbiihpo6EIQvKb1XIHo"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.divijakavishvar.cabby",   
      "versionCode": 1                          
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-location"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ada921ec-b3dc-402c-ba70-ab4fa949eb75"
      }
    }
  }
}
